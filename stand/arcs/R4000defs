CPUARCH_DEFS=-DR4000 $(CPUARCH_PRODDEFS)

EVEREST_X=\#
$(PRODUCT)_X=

CPUARCH_MD_COPTS=-D_PAGESZ=4096
CPUARCH_MD_ASOPTS=-mips3 -Wa,-r4000 -D_PAGESZ=4096

CPUARCH_MI_COPTS=$(SHARE_COPT) -elf -G 0 -o32
CPUARCH_MI_ASOPTS=$(SHARE_ASOPT) -elf -G 0 -o32
CPUARCH_MI_LDOPTS=-elf -G 0 -o32 -allow_jump_at_eop

SHARE_COPT=-non_shared
SHARE_ASOPT=-non_shared

DEF_CSTYLE=
OPTIMIZER=-O
GLDOPTS=
