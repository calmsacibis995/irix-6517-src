# Product definitions for IP22 with Mardi Gras (aka GR4) graphics.

SYSTEM = SVR4
GRAPHICS= GL4D3
ENDIAN =
CPUBOARD = IP22
COMPLEX = SP
GFXBOARD = MGRAS
SUBGR = MGRAS
CPUARCH = R4000
CPUARCH_PRODDEFS= -DR4000PC -DR4600 -DR4600SC -DTRITON -DTRITON_INDYSC
KN32PROM32= -D_KN32PROM32
KMODEOPT = $(KN32PROM32)
COMPILATION_MODEL=N32
include $(RELEASEDEFS)

