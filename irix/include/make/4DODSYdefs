# Product definitions for IPXX(Beast) with ODSY graphics.

SYSTEM = SVR4
GRAPHICS= GL4D3
CPUBOARD= IP30
COMPLEX = MP
ENDIAN=
GFXBOARD= ODYSSEY_FNC
SUBGR= ODYSSEY_FNC
CPUARCH= R10000
CPUARCH_PRODDEFS=
