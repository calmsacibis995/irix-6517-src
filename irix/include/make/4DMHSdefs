#
# Product definitions for IP32 machines with XXXX
# graphics defined.  This is used to build gfx/kern
# with support for XXX
# 
SYSTEM	= SVR4
GRAPHICS= GL4D3
CPUBOARD= IP32SIM
COMPLEX = SP
ENDIAN=
GFXBOARD= CRIME
SUBGR   = IP32
CPUARCH= R4000
CPUARCH_PRODDEFS=-DR4000PC -DR4600 -DR4600SC -DTRITON -D_SYSTEM_SIMULATION -D_NO_R4000
_R5000_CVT_WAR = 1
_NO_R4000 = 1

include $(RELEASEDEFS)
