include ${ROOT}/usr/include/make/commondefs
MAKERULES= ${DEPTH}/Makerules

METERING= -DMETERING
NDEBUG= -UNDEBUG

LCDEFS= ${NCDEFS} ${METERING} ${NDEBUG} -Dsgi
LCINCS= ${NCINCS} -I${INCLDIR}/netman
LCOPTS= ${NCOPTS}
LC++DEFS= ${NC++DEFS} ${METERING} ${NDEBUG} -Dsgi
LC++INCS= ${NC++INCS}  -I${INCLDIR}/netman  -I${INCLDIR}/netman/ui \
			-I${INCLDIR}/netman/event -I${INCLDIR}/tu

LC++OPTS= ${NC++OPTS}
LLDLIBS= ${NLDLIBS}

LIBDIR= ${DEPTH}/lib
LIBNAME= libnetman.a
LIBRARY= ${LIBDIR}/${LIBNAME}
LIBNETVIS= netman

LIBDIR_64= ${DEPTH}/lib/libnetman_64
LIBRARY_64= ${LIBDIR_64}/${LIBNAME}

LIBUIDIR= ${DEPTH}/ui/lib
LIBUINAME= libnetvisui.a
UILIBRARY= ${LIBUIDIR}/${LIBUINAME}
LIBNETVISUI= netvisui

LIBEVDIR= ${DEPTH}/event/lib
LIBEVNAME= libnetvisevent.a
EVLIBRARY= ${LIBEVDIR}/${LIBEVNAME}
LIBNETVISEVENT= netvisevent

NETVISLDLIBS= -l${LIBNETVIS} -lnetls -lnck -l${LIBNETVISUI} -l${LIBNETVISEVENT}

HEADERSIDB= HEADERS
HEADERSPATH= /usr/include/netman
LIBIDB= LIBRARY
LIBINSTPATH= /usr/lib32
DATAIDB= DATA
DATALINKSIDB= DATALINKS
DATAMANIDB= DATAMAN
DISPLAYIDB= DISPLAY
DISPLAYLINKSIDB= DISPLAYLINKS
DISPLAYMANIDB= DISPLAYMAN
APPDEFIDB= "${DISPLAYIDB} config(suggest)"
HELPIDB= HELP
GIFTSIDB= GIFTS
TUTORIALIDB= TUTORIAL
RELNOTESIDB= RELNOTES
PIDLIDB= PIDL

NV_INSTALL = /usr/NetVis
NV_INSTALL_APPDEF = ${NV_INSTALL}/app-defaults
NV_INSTALL_BIN = ${NV_INSTALL}/bin
NV_INSTALL_LIB = ${NV_INSTALL}/lib
NV_INSTALL_MIBS = ${NV_INSTALL_LIB}/mibs
NV_INSTALL_EVENT = ${NV_INSTALL_LIB}/events
NV_INSTALL_HELP = ${NV_INSTALL}/HelpCards
NV_INSTALL_ETC = ${NV_INSTALL}/etc
NV_INSTALL_MAN = ${NV_INSTALL}/man
NV_INSTALL_EXAMPLES = ${NV_INSTALL}/examples
NV_INSTALL_TUTORIAL = ${NV_INSTALL}/tutorial
NV_INSTALL_FTR = ${NV_INSTALL}/ftrs

NV_TARGET_USRETC = /usr/etc
NV_TARGET_ETC = /etc
NV_TARGET_BIN = /usr/sbin
NV_TARGET_LIB = /usr/lib/netman
NV_TARGET_APPDEF = /usr/lib/X11/app-defaults
NV_TARGET_MIBS = ${NV_TARGET_LIB}/mibs
NV_TARGET_MAN1 = /usr/catman/a_man/cat1
NV_TARGET_MAN7 = /usr/catman/a_man/cat7
NV_TARGET_HELPCARDS = /usr/lib/HelpCards
NV_TARGET_EXAMPLES = /usr/people/4Dgifts/examples/netman

UNSTRINGIFY= ${TOOLROOT}/usr/sbin/unstringify

sinclude ${DEPTH}/nlocaldefs

SNMP= SNMP
MANSNP= MANSNMP
FRIENDSNMP= FRIENDSNMP

