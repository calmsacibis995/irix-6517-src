!
!  NetTop Resources
!

! Controls file to read from
!NetTop*controlsFile:			~/.nettoprc

! hostresorder to use for name resolution
!NetTop*hostresorder:			local

! Use NIS for lookups
!NetTop*useyp:				False

! Force the use of colormap mode instead of rgb mode
!NetTop*colormap:			False

! X colors
!NetTop*nodeControlBackgroundColor:	#9fcaec
!NetTop*trafficControlBackgroundColor:	#9fcaec

! GL color; 0-255 each for blue, green, red,
! for running in rgb mode (if workstation has enough bitplanes)
!NetTop*mainBackgroundRGBColor:		0x000000
!NetTop*selectRGBColor:			0x00FFFF
!NetTop*labelRGBColor:			0x000000
!NetTop*nameRGBColor:			0x00FF00
!NetTop*nameErrorRGBColor:		0x0000FF
!NetTop*nameLockedRGBColor:		0xFF8000
!NetTop*baseRGBColor:			0x909090
!NetTop*scaleRGBColor:			0x00FF00
!NetTop*titleRGBColor:			0xFFFFFF
!NetTop*edgeRGBColor:			0x000000

! GL color map indices, for running in colormap mode
!   (if workstation has too few bitplanes to run in RGB mode)
!NetTop*mainBackgroundIndexColor:	0
!NetTop*selectIndexColor:		3
!NetTop*labelIndexColor:		0
!NetTop*nameIndexColor:			2
!NetTop*nameErrorIndexColor:		1
!NetTop*nameLockedIndexColor:		4
!NetTop*baseIndexColor:			15
!NetTop*scaleIndexColor:		2
!NetTop*titleIndexColor:		7
!NetTop*edgeIndexColor:			0
!NetTop*totalTotalIndexColor:		5
!NetTop*otherOtherIndexColor:		9
!NetTop*totalAnyIndexColor:		12
!NetTop*otherAnyIndexColor:		13
!NetTop*anyAnyIndexColor:		4
