	# Selected
	click	window:Desk+1 x:20 y:40

	sleep	wait:1

	# Selected->Open Icon
	click	window:Desk+1 x:120 y:45 time:1

$	Open Folder (2nd)
	sleep	wait:5

	# Double click the Empty.dir folder
	doubleclick	window:/usr/people/guest x:75 y:135 time:1

	# Clean up Empty.dir
	sleep	wait:1
	doubleclick	window:/usr/people/guest/Empty.dir x:5 y:-10

	# Clean up guest
	sleep	wait:1
	doubleclick window:/usr/people/guest x:5 y:-10
