# Makedefs for SNMP subdirectories
#
#ident "$Revision: 1.1 $"

DEPTH= ../${SNMPDEPTH}
include ${DEPTH}/Makedefs

CSTYLE=

SNMPDIR= ${SNMPDEPTH}


SNMPLIBDIR= ${SNMPDIR}/lib/libsnmp_64
SNMPLIBNAME=libsnmp.a
SNMPLIBRARY= ${SNMPLIBDIR}/${SNMPLIBNAME}
SNMPLIBRARY_64= ${SNMPLIBDIR}/${SNMPLIBNAME}

AGENTLIBDIR= ${SNMPDIR}/libagent/libsnmpagent_64
AGENTLIBNAME=libsnmpagent.a
AGENTLIBRARY= ${AGENTLIBDIR}/${AGENTLIBNAME}
AGENTLIBRARY_64= ${AGENTLIBDIR}/${AGENTLIBNAME}

NCINCS= -I${SNMPDIR}/include -I${AGENTLIBDIR}
NC++INCS= -I${SNMPDIR}/include -I${AGENTLIBDIR}
NC++DEFS= -fullwarn
NC++OPTS= -64 -mips3

sinclude ./nlocaldefs
