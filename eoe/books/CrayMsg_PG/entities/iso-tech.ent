<!ENTITY or     SDATA "font=symbol charset=fontspecific code=218 
			descr='[or]'"--/vee /lor B: =logical or-->
<!ENTITY par    SDATA "TeX='\| ' descr='[par]'"--/parallel R: =parallel-->
<!ENTITY part   SDATA "font=symbol charset=fontspecific code=182
			descr='[part]'"--/partial =partial differential-->
<!ENTITY perp   SDATA "font=symbol charset=fontspecific code=94  
