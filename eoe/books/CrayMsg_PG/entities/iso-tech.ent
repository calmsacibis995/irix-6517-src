-->
<!ENTITY aleph  SDATA "font=symbol charset=fontspecific code=192
			descr='[aleph]'"--/aleph =aleph, Hebrew-->
<!ENTITY and    SDATA "font=symbol charset=fontspecific code=217 
			descr='[and]'"--/wedge /land B: =logical and-->
<!ENTITY ap     SDATA "font=symbol charset=fontspecific code=64  
