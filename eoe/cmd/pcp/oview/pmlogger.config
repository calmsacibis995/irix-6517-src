# pmlogger(1) configuration file suitable creating an archive to be
# used with oview(1)
#
# $Id: pmlogger.config,v 1.3 1999/04/30 04:03:45 kenmcd Exp $

log mandatory on once { hinv }

log advisory on default {
    hw.router
    origin.node
    kernel.percpu.cpu
    origin.numa.migr.intr.total
}
