#
#	IRIX /etc/netconfig
#
#netid    semantics   flag family  proto   device       lookups...
#
ticlts    tpi_clts      v  loopback -    /dev/ticlts    /usr/lib32/straddr.so,/usr/lib/straddr.so
ticots    tpi_cots      v  loopback -    /dev/ticots    /usr/lib32/straddr.so,/usr/lib/straddr.so
ticotsord tpi_cots_ord  v  loopback -    /dev/ticotsord /usr/lib32/straddr.so,/usr/lib/straddr.so
tcp       tpi_cots_ord	v  inet     tcp  /dev/tcp	/usr/lib32/tcpip.so,/usr/lib/tcpip.so
udp       tpi_clts	bv inet	    udp  /dev/udp	/usr/lib32/tcpip.so,/usr/lib/tcpip.so
icmp      tpi_raw       -  inet     icmp /dev/icmp	/usr/lib32/tcpip.so,/usr/lib/tcpip.so
rawip     tpi_raw       -  inet     -    /dev/rawip	/usr/lib32/tcpip.so,/usr/lib/tcpip.so
